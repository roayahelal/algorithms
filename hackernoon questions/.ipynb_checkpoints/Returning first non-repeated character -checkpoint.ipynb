   "source": [
    "#1 find the first non repeated character of a string\n",
    "\n",
    "def findnonrep(string):\n",
    "    char = []\n",
    "    for i in string:\n",
    "        char.append(i)\n",
    "    for i in range(0,len(char)):\n",
    "        if char[i] != char[i -1]:\n",
    "            return char[i]"
   ]
  },
